    "import numpy as np\n",
    "\n",
    "def leibniz(termNum):\n",
    "    n = 0\n",
    "    term = 0\n",
    "    summation = 0\n",
    "    while n < termNum:\n",
    "        term = ((-1)**n) / (2*n + 1)\n",
    "        summation += term\n",
    "        n += 1\n",
    "    return summation*4\n",
    "\n",
    "print(leibniz(500000))\n",
